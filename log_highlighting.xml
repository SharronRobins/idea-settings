<application>
  <component name="LogHighlightingSettings">
    <Patterns>
      <list>
        <LogHighlightingPattern pattern="^\s*e(rror)?\s*$" action="HIGHLIGHT_LINE" fg="-65536" bold="true" stripe="true" />
        <LogHighlightingPattern pattern="^\s*w(arning)?\s*$" action="HIGHLIGHT_LINE" fg="-22016" bold="true" />
        <LogHighlightingPattern pattern="^\s*i(nfo)?\s*$" action="HIGHLIGHT_LINE" fg="-12599489" />
        <LogHighlightingPattern pattern="ERROR:" action="HIGHLIGHT_LINE" fg="-1835006" bold="true" stripe="true" />
        <LogHighlightingPattern pattern="ErrorException:" action="HIGHLIGHT_LINE" fg="-1802929" bold="true" stripe="true" />
      </list>
    </Patterns>
    <hidden>
      <list>
        <option value="#\d+" />
        <option value="#\d+" />
        <option value="#0" />
        <option value="0" />
        <option value="migrate:refresh" />
      </list>
    </hidden>
  </component>
</application>