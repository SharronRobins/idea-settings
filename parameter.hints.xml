<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="PHP">
        <added pattern="\XMLWriter.writeElement(name,content)" />
        <added pattern="\FormFiveXMLBuilder.mapToXML(type,value)" />
        <added pattern="\FormFiveXMLBuilder.writeAddressBlock(field,timeField)" />
        <added pattern="\Form.addErrorMessage(fieldName,message,messageType,escapeHtml)" />
        <added pattern="\compact(varname,_)" />
        <added pattern="\array_diff(array1,array2,_)" />
        <added pattern="\date(format,timestamp)" />
        <added pattern="\sprintf(format,args,_)" />
        <added pattern="\DataList.limit(limit,offset)" />
        <added pattern="\CMSMain.LinkPageAdd(extra,placeholders)" />
        <added pattern="\Illuminate\Routing\Route.name(name)" />
        <added pattern="\DateTime.format(format)" />
        <added pattern="\Illuminate\Routing\Route.middleware(middleware)" />
        <added pattern="\Illuminate\Support\Facades\Route.get(uri,action)" />
        <added pattern="\Illuminate\Routing\Route.uses(action)" />
        <added pattern="\Illuminate\Database\Eloquent\Builder.where(column,operator,value,boolean)" />
        <added pattern="\DataObject.dbObject(fieldName)" />
      </blacklist>
    </blacklists>
  </component>
</application>