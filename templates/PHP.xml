<templateSet group="PHP">
  <template name="upload_field" value="UploadField::create('$NAME$')-&gt;setFolderName('$PARENT$s/$CHILD$')" description="Silverstripe Upload Field" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARENT" expression="" defaultValue="NAME" alwaysStopAt="true" />
    <variable name="CHILD" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="__const" value="public function __construct(){&#10;&#9;$END$&#10;&#9;&#10;}" description="PHP Constructor" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pris" value="private static $$$NAME$ = $END$;" description="private static" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="/#" value="/************************************************************************&#10; * $END$&#10; ************************************************************************/" description="Section Comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="isa" value="isset($SELECTION$) &amp;&amp; $SELECTION$$END$" description="isset(?) &amp;&amp; ?" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="se" value="#region $NAME$&#10;$writer-&gt;startElement('$NAME$');&#10;    $END$&#10;$writer-&gt;endElement();&#10;#endregion" description="$writer-&gt;startElement" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
</templateSet>